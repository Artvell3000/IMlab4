<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <HBox maxHeight="500.0" maxWidth="500.0" minHeight="500.0" minWidth="500.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Pane fx:id="mainPane" style="-fx-background-color: white;" />
         </children>
      </HBox>
      <HBox maxWidth="180.0" prefHeight="24.0" prefWidth="203.0" spacing="10.0">
         <children>
            <Button fx:id="btnStartStop" mnemonicParsing="false" text="Start" onAction="#clickStartStop"/>
            <Button fx:id="btnDraw" mnemonicParsing="false" text="draw" onAction="#clickDraw"/>
            <Button fx:id="btnClear" mnemonicParsing="false" text="clear" onAction="#clickClear"/>
         </children>
      </HBox>
   </children>
</VBox>
